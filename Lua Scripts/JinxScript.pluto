					toast($"[Anti-Cage] Deleting an object...\nModel Name: {reverse_joaat(GET_ENTITY_MODEL(obj))}", TOAST_ALL)
					break
				case 3:
					SET_ENTITY_ALPHA(obj, 50, false)
					toast($"[Anti-Cage] Deleting an object...\nModel Name: {reverse_joaat(GET_ENTITY_MODEL(obj))}", TOAST_ALL)
